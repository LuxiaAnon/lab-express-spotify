<section>
    <h2>Here the albums of your artist:</h2>
    <section id="albums">
        {{#each albums}}
        <div>
            <img src="{{this.images.[1].url}}" alt="{{this.name}}">
            <h3>{{this.name}}</h2>
            <a href="/tracks/{{this.name}}/{{this.id}}">View tracks</a>
        </div>
        {{/each}}
    </section>
</section>